{
  "author": "https://github.com/livanynunes, https://github.com/tuliosj",
  "name": "track-matcher-for-spotify",
  "description": "An app to match songs in common between profiles",
  "version": "1.0.0",
  "scripts": {
    "client": "cd client && npm start",
    "server": "node server.js",
    "start": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "postinstall": "cd client && npm install"
  },
  "dependencies": {
    "@types/node": "^14.0.1",
    "concurrently": "^5.2.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.4",
    "cryptiles": ">=4.1.3",
    "dotenv": "^8.2.0",
    "express": "~4.16.0",
    "nodemon": "^2.0.3",
    "querystring": "~0.2.0",
    "request": "~2.83.0",
    "request-promise": "^4.2.5",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.2"
  }
}
